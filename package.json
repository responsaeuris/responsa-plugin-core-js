{
  "name": "responsa-plugin-core-js",
  "version": "0.2.2",
  "description": "Responsa Plugins Core",
  "main": "index.js",
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "scripts": {
    "do_not_run": "echo \"updating outer package.json\" && npm i shx && shx cp package.json ../package.json && echo \"Done!\"",
    "do_not_run2": "shx sed -i \"s/do_not_run/do_not_run/g\" ../package.json && shx sed -i \"s/do_not_run2/do_not_run2/g\" ../package.json",
    "test": "npm run lint && npm run jest",
    "lint": "standard ./",
    "jest": "jest --detectOpenHandles --watchAll",
    "jest:ci": "jest --forceExit --ci --testResultsProcessor=jest-multiple-result-processors --coverage=true",
    "start": "node ./examples/basic/server.js",
    "dev": "PORT=3100 node ./examples/basic/server.js",
    "debug": "PORT=3100 node ./examples/basic/server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://stash.getconnected.it/scm/rspevo/plugin-core-js.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@elastic/elasticsearch": "^7.10.0",
    "dotenv": "^9.0.0",
    "fastify": "^3.0.0",
    "fastify-autoload": "^3.0.2",
    "fastify-error": "^0.2.0",
    "fastify-oas": "^3.0.8",
    "fastify-plugin": "^3.0.0",
    "fastify-swagger": "^3.5.0",
    "node-cache": "^5.1.2",
    "pino": "^6.9.0",
    "pino-elasticsearch": "^5.4.0",
    "pino-multi-stream": "^5.1.1"
  },
  "devDependencies": {
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "jest-junit-reporter": "^1.1.0",
    "jest-multiple-result-processors": "^0.0.3",
    "jest-sonar-reporter": "^2.0.0",
    "shx": "^0.3.3",
    "standard": "^16.0.3"
  },
  "jestTestResultProcessors": [
    "jest-junit-reporter",
    "jest-sonar-reporter"
  ],
  "jestSonar": {
    "reportPath": ".",
    "reportFile": "test-report-sonar.xml",
    "indent": 4
  },
  "standard": {
    "env": [
      "jest"
    ],
    "globals": [
      "describe",
      "test"
    ]
  },
  "standard.options": {
    "ignore": [
      "node_modules/**"
    ],
    "parser": "babel-eslint"
  },
  "jest": {
    "testEnvironment": "node",
    "testTimeout": 15000
  }
}
